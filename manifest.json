{
  "manifest_version": 3,
  "name": "Social Media Launcher",
  "description": "Quickly launch your social media accounts",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
